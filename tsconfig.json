{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2019",
      "rootDir": "./src",
      "outDir": "./build",
      "sourceMap": false,
      "strict": true,
      "noImplicitReturns": true,
      "noImplicitAny": true,
      "skipLibCheck": true,
      "strictNullChecks": true
    },
    "exclude": [
      "node_modules",
    ],
    "include": [
      "src/**/*"
    ]
  }